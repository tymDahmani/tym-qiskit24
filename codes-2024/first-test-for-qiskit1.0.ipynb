   "source": [
    "from qiskit import *\n",
    "qc = QuantumCircuit(2,2)\n",
    "qc.x(0)\n",
    "qc.measure(0,0)\n",
    "\n",
    "sim = Aer.get_backend('qasm_simulator')\n",
    "result = execute(qc, backend=sim).result()\n",
    "counts = result.get_counts(qc)\n",
    "print(counts)"
   ]
